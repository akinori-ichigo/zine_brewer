require 'rubygems'

version = ">= 0.a"

str = ARGV.first
if str
  str = str.b[/\A_(.*)_\z/, 1]
  if str and Gem::Version.correct?(str)
    version = str
    ARGV.shift
  end
end

if Gem.respond_to?(:activate_bin_path)
load Gem.activate_bin_path('zine_brewer', 'zine_brewer', version)
else
gem "zine_brewer", version
load Gem.bin_path("zine_brewer", "zine_brewer", version)
end
